[sub_resource type="GDScript" id="GDScript_747gg"]
script/source = "extends Node2D


func _ready() -> void:
	if(\"web\" in OS.get_name().to_lower() or true):
		$notice/Button.disabled = false
		$AnimationPlayer.play(\"notice_in\")
		
		


func _on_animation_player_animation_finished(anim_name: StringName) -> void:
	if(anim_name == \"noitce_out\"):
		$AnimationPlayer.play(\"menu_in\")


func _on_button_pressed() -> void:
	print(\"press\")
	$AnimationPlayer.play(\"notice_out\")
"

