[sub_resource type="GDScript" id="GDScript_747gg"]
script/source = "extends Node2D


func _ready() -> void:
	if(\"web\" in OS.get_name().to_lower()):
		$notice/Button.disabled = false
		$AnimationPlayer.play(\"notice_in\")
	else:
		await get_tree().create_timer(2).timeout
		$AnimationPlayer.play(\"menu_in\")
		
		
func _input(event: InputEvent) -> void:
	if(event.is_action_released(\"click\")):
		if($notice/Button.get_global_rect().has_point(get_global_mouse_position()) and not $notice/Button.disabled):
			_on_button_pressed()
		elif($menu/TextureButton.get_global_rect().has_point(get_global_mouse_position())):
			$AnimationPlayer.play(\"fadeout\")

func _on_animation_player_animation_finished(anim_name: StringName) -> void:
	if(anim_name == \"notice_out\"):
		$AnimationPlayer.play(\"menu_in\")
	if(anim_name == \"fadeout\"):
		get_tree().call_deferred(\"change_scene_to_file\",\"res://story.tscn\")
		

func _on_button_pressed() -> void:
	print(\"press\")
	$AnimationPlayer.play(\"notice_out\")
"

